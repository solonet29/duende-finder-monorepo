{
  "name": "duende-finder-monorepo",
  "private": true,
  "packageManager": "pnpm@9.6.0",
  "version": "1.0.0",
  "description": "Monorepo para el proyecto de Duende Finder",
  "scripts": {
    "dev": "pnpm --filter \"./apps/*\" dev",
    "build": "pnpm --filter \"./apps/*\" build",
    "start": "pnpm --parallel --filter \"./apps/*\" start",
    "lint": "pnpm --filter \"./apps/*\" lint",
    "test": "pnpm --filter \"./apps/*\" test"
  },
  "keywords": [
    "monorepo",
    "pnpm",
    "duende-finder"
  ],
  "author": "",
  "license": "ISC",
  "engines": {
    "node": "20.x"
  },
  "devDependencies": {
    "@types/node": "^24.6.2",
    "dotenv": "^16.4.5",
    "mongodb": "^6.8.0"
  }
}