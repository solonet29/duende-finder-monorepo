{
    "headers": [
        {
            "source": "/api/config",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "s-maxage=43200, stale-while-revalidate=3600"
                },
                {
                    "key": "Access-Control-Allow-Credentials",
                    "value": "true"
                },
                {
                    "key": "Access-Control-Allow-Origin",
                    "value": "https://buscador.afland.es"
                },
                {
                    "key": "Access-Control-Allow-Methods",
