{
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "https://duende-api.vercel.app/api/$1"
    },
    {
      "src": "/manifest.json",
      "dest": "/manifest.json",
      "headers": {
        "Content-Type": "application/manifest+json"
      }
    },
    {
      "src": "/assets/manifest.json",
      "dest": "/assets/manifest.json",
      "headers": {
        "Content-Type": "application/manifest+json"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}